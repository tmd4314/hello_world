<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.mapper.MemberMapper">
	<select id="selectMember" resultType="member">
		select user_id,
		       password,
		       user_name,
		       responsibility
		from   tbl_member
		where  user_id = #{id}
		  and  password = #{pw}
	</select>
</mapper>